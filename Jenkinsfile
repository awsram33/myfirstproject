pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Welcome to the Pipeline'
            }
        }
        stage('Build') {
            steps {
                echo 'Building Java Project'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing Java Project'
            }
        }
        stage('Package') {
            steps {
                echo 'Packing Java Project'
            }
        }
    }
}
